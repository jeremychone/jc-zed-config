[
	// -- Line Management
	// Add line below
	{
		"context": "Editor",
		"bindings": {
			"cmd-shift-enter": [
				"action::Sequence",
				["editor::NewlineAbove", "editor::MoveDown", "editor::MoveToEndOfLine"],
			],
		},
	},
	// Add line below
	{
		"context": "Editor",
		"bindings": {
			"ctrl-cmd-enter": [
				"action::Sequence",
				["editor::NewlineBelow", "editor::MoveUp", "editor::MoveToEndOfLine"],
			],
		},
	},
	// Delete line
	{
		"context": "Editor",
		"bindings": {
			"cmd-backspace": "editor::DeleteLine",
		},
	},
	// Move Line & Indent
	{
		"context": "Editor",
		"bindings": {
			"cmd-[": "editor::MoveLineDown",
			"cmd-]": "editor::MoveLineUp",
			"ctrl-alt-]": "editor::Indent",
			"ctrl-alt-[": "editor::Outdent",
		},
	},
	// End line with ;
	{
		"context": "Editor",
		"bindings": {
			"cmd-;": [
				"action::Sequence",
				["editor::MoveToEndOfLine", ["workspace::SendKeystrokes", ";"]],
			],
		},
	},

	// -- In zed send r to term
	{
		"bindings": {
			"ctrl-alt-r": ["workspace::SendKeystrokes", "ctrl-` r"],
		},
	},

	// -- Tasks
	// Start dev term (default, below)
	{
		"bindings": {
			"ctrl-cmd-j": [
				"task::Spawn",
				{
					"task_name": "Dev Term Below",
					// "reveal_target": "center",
				},
			],
		},
	},
	// Start dev term (default, below)
	{
		"bindings": {
			"ctrl-cmd-shift-j": [
				"task::Spawn",
				{
					"task_name": "Dev Term Bottom",
					// "reveal_target": "center",
				},
			],
		},
	},
	// API Run
	{
		"bindings": {
			"cmd-r": [
				"task::Spawn",
				{
					"task_name": "AIP RUN",
					// "reveal_target": "center",
				},
			],
		},
	},
	// TOGGLE AI
	// API Run
	{
		"bindings": {
			"ctrl-cmd-a": [
				"task::Spawn",
				{
					"task_name": "Toggle AI",
				},
			],
		},
	},

	// -- Surround
	// <>
	{
		"context": "Editor",
		"bindings": {
			"ctrl-alt-,": ["workspace::SendKeystrokes", "cmd-x < cmd-v >"],
		},
	},
	// ()
	{
		"context": "Editor",
		"bindings": {
			"ctrl-alt-9": ["workspace::SendKeystrokes", "cmd-x ( cmd-v )"],
		},
	},

	// -- cmd+shift+g - Git panel Override
	{
		"context": "Global || Editor || Workspace",
		"bindings": {
			"cmd-shift-g": "git_panel::ToggleFocus",
		},
	},

	// -- JC Custom Pante
	{
		"bindings": {
			"ctrl-cmd-right": "pane::SplitAndMoveRight",
		},
	},

	// -- Toggle inlay
	{
		"context": "!ContextEditor > (Editor && mode == full)",
		"bindings": {
			"cmd-u": "editor::ToggleInlayHints",
		},
	},

	// -- Pame Management
	{
		"context": "Workspace",
		"bindings": {
			"alt-cmd-r": "workspace::ToggleRightDock",
		},
	},
	{
		"context": "Pane",
		"bindings": {
			"alt-g": "search::SelectNextMatch",
		},
	},
	{
		"context": "Pane",
		"bindings": {
			"alt-shift-g": "search::SelectPreviousMatch",
		},
	},

	// -- JC Remaping Opens
	{
		"bindings": {
			"alt-cmd-o": "workspace::Open",
		},
	},
	// JC: Because I always do this. Rather if ever the workspace::Open
	{
		"bindings": {
			"cmd-o": [
				"projects::OpenRecent",
				{
					"create_new_window": false,
				},
			],
		},
	},
	{
		"bindings": {
			"ctrl-cmd-z": "workspace::ToggleZoom",
		},
	},
 {
  "bindings": {
   "alt-r": "editor::ReloadFile"
  }
 }
]
